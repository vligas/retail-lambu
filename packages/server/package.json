{
  "name": "server",
  "version": "0.0.0",
  "description": "servidor de la aplicacion retail",
  "author": "",
  "license": "MIT",
  "scripts": {
    "migrate": "npm run clean && npm run build && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/build/migrate.js",
    "seed": "npm run clean && npm run build && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/build/seed.js",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "webpack": "webpack --config webpack.config.js",
    "start:hot": "node dist/server",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch --onSuccess \"node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/main.js\" --onFailure \"echo Beep! Compilation Failed\" --compiler typescript/bin/tsc",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/main.js",
    "clean": "rimraf dist",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.1.1",
    "@nestjs/core": "^6.1.1",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.1.1",
    "@nestjs/swagger": "^3.0.2",
    "@types/dotenv": "^6.1.1",
    "@types/joi": "^14.3.3",
    "@types/sequelize": "^4.27.49",
    "@types/umzug": "^2.2.1",
    "@types/winston": "^2.4.4",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^8.0.0",
    "fs-extra": "^7.0.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-request-context": "^1.0.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.10.0",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.1",
    "sequelize": "^5.8.5",
    "sequelize-typescript": "^1.0.0-beta.3",
    "swagger-ui-express": "^4.0.2",
    "tedious": "^6.1.1",
    "typescript": "^3.4.5",
    "umzug": "^2.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.1.1",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.1.0",
    "@types/jest": "^24.0.12",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/multer": "^1.3.7",
    "@types/node": "^12.0.0",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.7",
    "@types/webpack": "^4.4.31",
    "faker": "^4.1.0",
    "fs-extra": "^7.0.1",
    "jest": "^24.8.0",
    "ng-faker": "^1.1.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.0",
    "ts-node": "^7.0.0",
    "tsc-watch": "^2.1.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.16.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}